suits = ["S", "H", "C", "D"]
card_value = ["Ace", "2", "3", "4", "5", "6", "7", "8", "9", "10", "Jack", "Queen", "King"]

class Card:
	def __init__(self, suite, value):
		self.suite = suite
		self.value = value

class Deck:
    def __init__(self):
        self.cards = []

deck=[Card(suits, card_value) for value in card_value for suite in suits]

print(list(deck))

